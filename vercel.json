{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ],
    "routes": [
      {
        "src": "/sitemap.xml",
        "dest": "/public/sitemap.xml"
      }
    ]
  }
  